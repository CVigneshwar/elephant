{
  "name": "HighSchool Scheduler Dev",
  "dockerFile": "Dockerfile",
  "context": "..",
 
  "settings": {
    "files.autoSave": "onFocusChange",
    "terminal.integrated.shell.linux": "/bin/bash"
  },
 
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "redhat.java",
        "angular.ng-template",
        "esbenp.prettier-vscode"
      ]
    }
  },
 
  "postCreateCommand": "npm install --prefix scheduler-ui"
}